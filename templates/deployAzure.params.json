{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "unique": {
      "value": "<your_unique_string>"
    },
    "servicePrincipalAppId": {
      "value": "<user_object_id>"
    },
    "jumpserverUser": {
      "value": "azureuser"
    },
    "jumpserverSSHKey": {
      "value": "<ssh_public_key>"
    },
    "jumpserverName": {
      "value": "jumpserver"
    },
    "jumpserverSize": {
      "value": "Standard_A1"
    },
    "vnetPrefix": {
      "value": "10.1.0.0/24"
    },
    "frontendPrefix": {
      "value": "10.1.0.0/25"
    },
    "backendPrefix": {
      "value": "10.1.0.128/26"
    },
    "dmzPrefix": {
      "value": "10.1.0.192/28"
    },
    "managePrefix": {
      "value": "10.1.0.208/28"
    },
    "remoteAccessACL": {
      "value": "Internet"
    },
    "backendServerNamePrefix": {
      "value": "db"
    },
    "backendServerSize": {
      "value": "Standard_DS1_v2"
    },
    "scaleSetInstanceCount": {
      "value": 2
    }
  }
}
